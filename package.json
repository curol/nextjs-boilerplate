{
  "name": "nextjs-experimental",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind": "tailwindcss -o dist/tailwind.css --minify",
    "eslint": "eslint . --ext js,jsx,ts,tsx --max-warnings=0 --config .eslintrc",
    "eslint-fix": "eslint . --ext js,jsx,ts,tsx --max-warnings=0 --config .eslintrc --fix",
    "eslint-config": "eslint --print-config .eslintrc",
    "format": "prettier --write --ignore-unknown .",
    "format-check": "prettier --check --ignore-unknown .",
    "types": "tsc --noEmitOnError",
    "type-check": "tsc --noEmit",
    "test": "jest --watch",
    "test:ci": "jest --ci"
  },
  "dependencies": {
    "@headlessui/react": "latest",
    "@heroicons/react": "latest",
    "@next/font": "latest",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest",
    "zustand": "^4.3.3"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "latest",
    "@tailwindcss/forms": "latest",
    "@tailwindcss/typography": "latest",
    "@testing-library/dom": "latest",
    "@testing-library/jest-dom": "latest",
    "@testing-library/react": "latest",
    "@testing-library/user-event": "latest",
    "@types/jest": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "autoprefixer": "latest",
    "daisyui": "latest",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "jest": "latest",
    "jest-environment-jsdom": "latest",
    "msw": "latest",
    "postcss": "latest",
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "latest",
    "tailwindcss": "latest",
    "typescript": "latest",
    "vercel": "^28.13.2"
  }
}
